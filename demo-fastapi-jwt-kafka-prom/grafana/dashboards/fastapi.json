{
  "title": "FastAPI Demo Observability",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "panels": [
    {
      "type": "stat",
      "title": "App UP (scrape)",
      "targets": [
        {
          "expr": "up{job=\"fastapi\"}"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP Requests (rate)",
      "targets": [
        {
          "expr": "rate(http_requests_total[1m])"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 6,
        "y": 0
      }
    },
    {
      "type": "timeseries",
      "title": "Request Duration p95",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      }
    },
    {
      "type": "timeseries",
      "title": "CPU seconds",
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total[1m])"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    }
  ]
}